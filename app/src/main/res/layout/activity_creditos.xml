        android:id="@+id/imageView4"
        android:layout_width="97dp"
        android:layout_height="102dp"
        android:layout_marginTop="112dp"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.551"
